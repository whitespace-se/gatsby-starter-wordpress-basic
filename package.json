{
  "name": "@whitespace/gatsby-starter-wordpress-basic",
  "private": true,
  "description": "Gatsby starter for Wordpress",
  "version": "0.0.0",
  "author": "Whitespace <code@whitespace.se>",
  "license": "AGPL",
  "scripts": {
    "build": "yarn wp-generate-schema-fragments && gatsby build",
    "clean": "gatsby clean",
    "dev": "yarn wp-generate-schema-fragments && gatsby develop --open",
    "format": "run-s format:*",
    "format:eslint": "npx eslint --fix '**/*.js' || true",
    "format:prettier": "prettier --write \"**/*.{js,jsx,json,md,scss,css,yml,html}\"",
    "refresh": "curl -X POST http://localhost:8000/__refresh",
    "serve": "gatsby serve",
    "start": "yarn dev",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "wp-generate-schema-fragments": "node scripts/wp-generate-schema-fragments.js"
  },
  "dependencies": {
    "@wsui/base": "^0.2.12",
    "@wsui/theme-standard": "^0.2.12",
    "@whitespace/gatsby-plugin-cookie-consent": "^1.4.0-next.5",
    "@whitespace/gatsby-plugin-matomo": "^1.4.0-next.5",
    "@whitespace/gatsby-theme-wordpress-basic": "^1.4.0-next.5",
    "formik": "^2.2.9",
    "gatsby": "^3.14.3",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react": "^7.26.1",
    "http-proxy-middleware": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1"
  },
  "resolutions": {
    "@apollo/client": "3.5.10",
    "postcss": "^8",
    "postcss-preset-env": "^7"
  }
}
